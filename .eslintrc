{
  	"parser": "@babel/eslint-parser",
	"parserOptions": {
	    "ecmaVersion": 8,
	    "requireConfigFile": false,
	    "ecmaFeatures": {
	    	"experimentalObjectRestSpread": true,
	    	"impliedStrict": true,
	    	"classes": true
		}
	},
  	"rules": {
    	"no-unused-vars": "warn"
  	},
  	"env": {
   		"es6": true,
    	"browser": true
  	},
  	"extends": [
    	"eslint:recommended"
  	]
}